#include<stdio.h>
int main(){
    int num1,num2,num3;
    printf("enter the numer 1:");
    scanf("%d",&num1);
    printf("enter the numer 2:");
    scanf("%d",&num2);
    printf("enter the numer 3:");
    scanf("%d",&num3);

    if(num1>num2&& num1>num3){
        printf("number one is greatest");
    }else if(num2>num1&& num2>num3){
        printf("number two is greatest");
    }else{
        printf("number three is greatest ");
}
return 0;
}
