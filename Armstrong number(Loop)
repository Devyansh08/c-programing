#include<stdio.h>
#include<math.h>
int main(){
    int num,result;
    printf("Please enter a number: ");
    scanf("%d",&num);
    int a=num;
    while(num>0){
        num%=10;
        num=num/10;
        result=pow(num,3);
    }
    result==a?printf("It is a armstrong number"):printf("It is not a armstrong number");
    return 0;

}
